<p id="notice"><%= notice %></p>

<h1>Feeds</h1>

<table>
  <thead>
    <tr>
      <th>Image</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @feeds.each do |feed| %>
      <tr>
        <td><%= image_tag feed.image.url %></td>
        <td><%= link_to 'Show', feed %></td>

      </tr>
    <% end %>
  </tbody>
</table>

<br>
<%= link_to 'My favorites', favorites_path %> |
<%= link_to 'New Feed', new_feed_path %>
